{
  "author": "Ed Charbeneau",
  "classifications": [
    "Web",
    "MVC",
    "SPA",
    "Kendo"
  ],
  "groupIdentity": "Progress.AspNetCore.SpaTemplates.Kendo.Angular.CSharp",
  "identity": "Microsoft.AspNetCore.SpaTemplates.Angular.CSharp",
  "name": "MVC ASP.NET Core with Kendo UI for Angular",
  "preferNameDirectory": true,
  "primaryOutputs": [
    {
      "path": "WebApplicationBasic.csproj"
    }
  ],
  "shortName": "kendo-ng",
  "sourceName": "WebApplicationBasic",
  "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        ".template.config/**"
      ],
      "copyOnly": [
        "ClientApp/**",
        "wwwroot/**"
      ]
    }
  ],
  "symbols": {
    "sdkVersion": {
      "type": "bind",
      "binding": "dotnet-cli-version",
      "replaces": "sdkVersionInjectedHere"
    },
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "netcoreapp2.0",
          "description": "Target netcoreapp2.0"
        }
      ],
      "defaultValue": "netcoreapp2.0"
    }
  },
  "tags": {
    "language": "C#",
    "type": "project"
  }
}